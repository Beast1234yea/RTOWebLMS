# Environment Variables Template for Railway/Production
# Copy this to Railway dashboard as environment variables
# DO NOT commit actual .env file with real credentials

ASPNETCORE_ENVIRONMENT=Production
DATABASE_PROVIDER=PostgreSQL

# Supabase Connection String
# Format: Host=YOUR_HOST;Port=5432;Database=postgres;Username=YOUR_USERNAME;Password=YOUR_PASSWORD;SSL Mode=Require;Trust Server Certificate=true
ConnectionStrings__DefaultConnection=Host=aws-1-ap-southeast-2.pooler.supabase.com;Port=5432;Database=postgres;Username=postgres.YOUR_PROJECT_ID;Password=YOUR_NEW_PASSWORD;SSL Mode=Require;Trust Server Certificate=true

# Port (Railway sets this automatically)
PORT=8080
